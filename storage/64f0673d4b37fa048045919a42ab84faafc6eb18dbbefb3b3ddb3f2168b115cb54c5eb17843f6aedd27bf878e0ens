<script>
    StackExchange.ready(function () {

        $(document).on('click', '.js-theme-selector .js-enable-light-mode, .js-theme-selector .js-enable-dark-mode, .js-theme-selector .js-enable-system-mode', function (e) {
            e.preventDefault();

            var $this = $(this);
            $.post("/account/set-theme-preference",
                {
                    "fkey": StackExchange.options.user.fkey,
                    "accountId": StackExchange.options.user.accountId,
                    "theme": $this.data('theme')
                })
                .done(function (response) {
                    var theme = response.themeClass || '';

                    $('body').removeClass(function (i, oldClassList) {
                        return (oldClassList.match(/(^|\s)theme-\S+/g) || []).join(' ');
                    });
                    $('body').addClass(theme);
                    $('.js-theme-selector .js-enable-light-mode, .js-theme-selector .js-enable-dark-mode, .js-theme-selector .js-enable-system-mode').removeClass('theme-selected');
                    $this.addClass('theme-selected');
                })
                .fail(function (response) {
                    var error = JSON.parse(response.responseText).ErrorMessage;
                    StackExchange.helpers.showToast(error, { type: 'danger' });
                });
        });

        $(document).on('click', '.js-theme-selector .js-dismiss', function (e) {
            e.preventDefault();

            $.cookie('notice-dmb', '4;' + Date.now(), {
                expires: new Date(1588982400000),
                path: "/"
            });

            $('.js-theme-selector').fadeOutAndRemove();
        });
    });
</script>



